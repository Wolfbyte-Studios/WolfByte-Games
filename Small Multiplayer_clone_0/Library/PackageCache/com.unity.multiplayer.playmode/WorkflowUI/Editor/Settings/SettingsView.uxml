<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="SettingsView.uss"/>
    <ui:ScrollView name="ScrollView">
        <ui:Label text="Multiplayer Play Mode" class="title"/>
        <ui:Foldout text="Players">
            <ui:Toggle name="IsMppmActiveToggle" label="MPPM Enabled"/>
            <ui:Toggle name="ShowLaunchScreenToggle" label="Show Launch Screen"/>
            <ui:Toggle name="MutePlayersToggle" label="Mute Players"/>
            <ui:IntegerField name="AssetDatabaseRefreshTimeoutSlider" label="Asset Database Refresh Timeout (Seconds)" low-value="30" high-value="300"/>
        </ui:Foldout>
    </ui:ScrollView>
</ui:UXML>
